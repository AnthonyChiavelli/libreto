    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.2",
    "ts-node": "3.3.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/loglevel": "^1.6.3",
    "@types/mongodb": "3.5.0",
    "@types/node": "^16.6.1",
    "@types/query-string": "^6.3.0",
    "@types/webpack": "^5.28.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.1",
