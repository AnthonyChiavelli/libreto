  "scripts": {
    "watch": "nodemon ../dist/server/bundle.js --ext ts,tsx,js,jsx --watch ../server/src",
    "run": "node ../dist/server/bundle.js",
    "serve": "npm-run-all build run",
    "sw": "nodemon --exec yarn serve --ext ts,tsx,js,jsx --watch ../server/src",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier . --write --ignore-unknown",
    "clean": "npm-run-all lint prettier",
    "build": "webpack --config webpack.backend.js",
    "start": "nodemon --exec yarn serve --ext ts,tsx,js,jsx --watch ../server/src"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "4.17.1",
