    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "express-query-boolean": "^2.0.0",
    "loglevel": "^1.8.0",
    "mongodb": "3.5.0",
    "mongodb-client-encryption": "^1.2.7",
    "node": "^16.6.1",
    "nodemon": "^2.0.12",
    "objection": "^2.2.2",
    "prettier": "^2.3.2",
    "query-string": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.4.2",
    "webpack": "^5.50.0",
    "webpack-node-externals": "^2.5.0"
  }
